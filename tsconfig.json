{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@theme/*": ["src/theme/*"],
      "@features/*": ["src/features/*"],
      "@navigators/*": ["src/navigators/*"],
      "@routes/*": ["src/routes/*"],
      "@contexts/*": ["src/contexts/*"]
    }
  }
}
